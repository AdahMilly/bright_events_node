{
  "env": {
    "jest": true,
    "es6": true,
    "node": true
  },
 "defaultSeverity": "error",
 "extends": [
     "tslint:recommended"
 ],
 "linterOptions": {
   "exclude": [
      "coverage"
   ]
 },
 "jsRules": {},
 "rules": {
     "trailing-comma": [ false ],
     "quotemark": [true, "single"],
     "object-literal-sort-keys": [ false ],
     "interface-name": [true, "never-prefix"],
     "member-ordering": [ false ],
     "ordered-imports": [ false ],
     "whitespace": [ true, "check-module" ],
     "no-console": {
       "severity": "warning",
       "options": ["info", "log"]
     }
 },
 "rulesDirectory": []
}
